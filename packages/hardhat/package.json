{
  "name": "@se-2/hardhat",
  "version": "0.0.1",
  "scripts": {
    "account": "hardhat run scripts/generateAccount.ts",
    "chain": "hardhat node --network hardhat --no-deploy",
    "check-types": "tsc --noEmit --incremental",
    "compile": "hardhat compile",
    "deploy": "hardhat deploy --export-all ../nextjs/generated/deployedContracts.ts",
    "fork": "hardhat node --fork https://mainnet.infura.io/v3/460f40a260564ac4a4f4b3fffb032dad",
    "generate": "hardhat run scripts/generateAccount.ts",
    "lint": "eslint --config ./.eslintrc.json --ext .ts,.tsx .",
    "lint-staged": "lint-staged",
    "test": "hardhat test --network hardhat",
    "test:gas": "REPORT_GAS=true hardhat test --network hardhat",
    "test:trace": "TRACE=true hardhat test --network hardhat",
    "verify": "hardhat etherscan-verify"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.1",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.7",
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.11",
    "@nomicfoundation/hardhat-verify": "^2.0.10",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.3",
    "@types/chai": "^4.3.5",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "chai": "^4.3.7",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ethers": "^5.7.2",
    "hardhat": "^2.19.5",
    "hardhat-deploy": "^0.11.45",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-gas-reporter": "^1.0.9",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.3",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@allo-team/allo-v2-sdk": "^1.0.79",
    "@ethereum-attestation-service/eas-contracts": "^1.7.1",
    "@ethereum-attestation-service/eas-sdk": "^2.7.0",
    "@openzeppelin/contracts": "~5.0.2",
    "dotenv": "^16.4.5"
  }
}
